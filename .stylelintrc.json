{
  "extends": ["stylelint-config-standard-scss", "stylelint-config-clean-order"],
  "plugins": ["stylelint-order"],
  "rules": {
    "at-rule-empty-line-before": null,
    "no-duplicate-selectors": null,
    "order/order": [
      {
        "type": "at-rule",
        "name": "include"
      }
    ]
  }
}
